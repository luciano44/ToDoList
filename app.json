{
  "expo": {
    "name": "To Do List",
    "slug": "todoList",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/todo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e90ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.lucianoar.todolist",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/todo.png",
        "backgroundColor": "#1E90FF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
